{
    "geminicodeassist.project": "metro-markets-genai-code",
    "workbench.startupEditor": "none",
    "workbench.settings.applyToAllProfiles": [

    ],
    "vim.leader": " ",
    "vim.normalModeKeyBindings": [
      { "before": ["Q"], "after": ["g", "q"] },
      { "before": ["K"], "commands": ["editor.action.showHover"] },
      { "before": ["[", "b"], "after": ["g", "T"] },
      { "before": ["]", "b"], "after": ["g", "t"] },
      { "before": ["<C-u>"], "after": ["<C-u>", "z", "z"] },
      { "before": ["<C-d>"], "after": ["<C-d>", "z", "z"] },
      { "before": ["<Leader>", "["], "commands": ["editor.action.marker.prev"] },
      { "before": ["<Leader>", "]"], "commands": ["editor.action.marker.next"] },
      { "before": ["<Leader>", "/"], "commands": ["workbench.action.findInFiles"] },
      { "before": ["<Leader>", "a"], "commands": ["editor.action.quickFix"] },
      { "before": ["<Leader>", "s"], "commands": ["workbench.action.gotoSymbol"] },
      { "before": ["<Leader>", "b", "l"], "commands": ["workbench.debug.viewlet.action.toggleBreakpointsView"] },
      { "before": ["<Leader>", "b", "p"], "commands": ["editor.debug.action.toggleBreakpoint"] },
      { "before": ["<Leader>", "b", "x"], "commands": ["workbench.action.debug.start"] },
      { "before": ["<Leader>", "b", "X"], "commands": ["workbench.action.debug.run"] },
      { "before": ["<Leader>", "b", "t"], "commands": ["workbench.action.debug.stop"] },
      { "before": ["<Leader>", "d"], "commands": ["editor.action.showHover"] },
      { "before": ["<Leader>", "f", "a"], "commands": ["workbench.action.showCommands"] },
      { "before": ["<Leader>", "f", "b"], "commands": ["workbench.action.showAllEditors"] },
      { "before": ["<Leader>", "f", "s"], "commands": ["workbench.action.findInFiles"] },
      { "before": ["<Leader>", "f", "f"], "commands": ["workbench.action.quickOpen"] },
      { "before": ["<Leader>", "f", "u"], "commands": ["editor.action.referenceSearch.trigger"] },
      { "before": ["g", "1"], "after": ["`", "1"] },
      { "before": ["g", "2"], "after": ["`", "2"] },
      { "before": ["g", "3"], "after": ["`", "3"] },
      { "before": ["g", "4"], "after": ["`", "4"] },
      { "before": ["g", "5"], "after": ["`", "5"] },
      { "before": ["<Leader>", "v", "u"], "commands": ["git.sync"] },
      { "before": ["<Leader>", "v", "s"], "commands": ["workbench.view.scm"] },
      { "before": ["<Leader>", "v", "p"], "commands": ["git.push"] },
      { "before": ["<Leader>", "v", "b"], "commands": ["git.checkout"] },
      { "before": ["<Leader>", "v", "h"], "commands": ["git.openFileHistory"] },
      { "before": ["<Leader>", "v", "a"], "commands": ["git.annotate"] },
      { "before": ["<Leader>", "o", "l"], "commands": [ "editor.action.revealDefinitionAside"] },
      { "before": ["<Leader>", "p", "s"], "commands": ["workbench.action.showAllEditors"] },
      { "before": ["<Leader>", "p", "v"], "commands": ["workbench.view.explorer"] },
      { "before": ["<Leader>", "p", "f"], "commands": ["workbench.files.action.showActiveFileInExplorer"] },
      { "before": ["<Leader>", "p", "r"], "commands": ["workbench.action.openRecent"] },
      { "before": ["<Leader>", "r", "r"], "commands": ["editor.action.rename"] },
      { "before": ["<Leader>", "t"], "commands": ["workbench.action.terminal.toggleTerminal"] },
      { "before": ["<Leader>", "w"], "commands": ["editor.action.surroundWithSnippet"] },
      { "before": ["<Leader>", "x"], "commands": ["workbench.action.closeActiveEditor"] },
      { "before": ["<Leader>", "X"], "commands": ["workbench.action.closeAllEditors"] },
      { "before": ["g", "r"], "commands": ["editor.action.referenceSearch.trigger"] },
      { "before": ["g", "i"], "commands": ["editor.action.goToImplementation"] },
      { "before": ["g", "q"], "commands": ["workbench.action.closeSidebar"] },
      { "before": ["<F4>"], "commands": ["editor.toggleWordWrap"] },
    ],
    "vim.visualModeKeyBindings": [
      { "before": ["J"], "commands": ["editor.action.moveLinesDownAction"] },
      { "before": ["K"], "commands": ["editor.action.moveLinesUpAction"] },
      { "before": ["<Leader>", "y"], "commands": ["editor.action.clipboardCopyAction"] }
    ],
    "geminicodeassist.inlineSuggestions.enableAuto": true,
    "diffEditor.codeLens": true,
    "editor.minimap.enabled": false,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "git.blame.editorDecoration.enabled": true,
    "git.blame.statusBarItem.enabled": true,
    "catppuccin.accentColor": "blue",
    "editor.renderWhitespace": "none",
    "editor.lineNumbers": "relative",
    "explorer.confirmDelete": false,
    "git.openRepositoryInParentFolders": "never",
    "redhat.telemetry.enabled": false,
    "editor.fontSize": 14,
    "github.copilot.enable": {
      "*": true,
      "plaintext": false,
      "markdown": true,
      "scminput": false
    },
    "json.schemas": [],
    "github.copilot.nextEditSuggestions.enabled": false,
    "python.createEnvironment.trigger": "off",
    "terminal.integrated.defaultProfile.osx": "fish",
    "github.copilot.chat.codesearch.enabled": true
}